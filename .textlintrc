{
  "plugins": [
    "html"
  ],
  "filters": {},
  "rules": {
    "preset-ja-technical-writing": {
      "max-kanji-continuous-len": {
        "allow": [
          "自己実行無名関数",
          "即時実行関数式"
        ]
      }
    },
    "textlint-rule-ja-space-between-half-and-full-width": {
      "space": "always"
    },
    "prh": {
      "rulePaths": [
        "./prh.yml"
      ]
    }
  }
}
